        "e91ccce7-9616-4fef-8256-ea92f532d541": {
          "type": "headline",
          "settings": {
            "headline": "Movement is our Business",
            "headline_color": "",
            "headline_mobile_size": 30,
            "headline_size": 32
          }
        },
        "32e4bf52-d9a0-4634-85e4-40d46cbd8ba1": {
          "type": "body_copy",
          "settings": {
            "body_copy": "<p>People are not meant to sit still. We are all Meant to Move and that’s why Feetures, a family-owned business in North Carolina, was founded with the purpose of creating products to enhance your active lifestyle. After becoming the leading brand in the US run specialty market, we set our sights on becoming the most recognized performance sock brand in the world. We invite you to join us on our mission to inspire people to get moving. Together, we can go farther!<\/p>",
            "body_copy_color": "",
            "body_mobile_size": 16,
            "body_size": 18
          }
        }
