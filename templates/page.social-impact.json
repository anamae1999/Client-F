    "feetures_image_text_4xGKeY": {
      "type": "feetures-image-text",
      "blocks": {
        "desktop_image_dAD68L": {
          "type": "desktop_image",
          "settings": {
            "desktop_image": "shopify:\/\/shop_images\/367960285_3027629367374316_8889399434206420263_n.jpg"
