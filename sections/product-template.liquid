                  .cushion-bars {
                    display: flex;
                    margin: 20px 0;
                  }
                  .cushion-bar {
                    width: 100%;
                    height: 5px;
                    background-color: #bbcfd1;
                  }
                  .cushion-bar.active {
                    background-color: #15565b;
                  }

                  .cushion-bar:nth-child(2n) {
                    margin: 0 5px;
                  }
                </style>

                {% liquid
                  assign sock_height = product.metafields.custom.sock_height
                %}

