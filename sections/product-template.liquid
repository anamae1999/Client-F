                  <span class="accordion-plus">
                    <svg xmlns="http://www.w3.org/2000/svg" width="17.996" height="17.996" viewBox="0 0 17.996 17.996">
                      <g transform="translate(-1751.504 -1172.5)"><path d="M0,17a1,1,0,0,1-1-1V0A1,1,0,0,1,0-1,1,1,0,0,1,1,0V16A1,1,0,0,1,0,17Z" transform="translate(1760.502 1173.5)"/><path d="M0,17a1,1,0,0,1-1-1V0A1,1,0,0,1,0-1,1,1,0,0,1,1,0V16A1,1,0,0,1,0,17Z" transform="translate(1768.5 1181.498) rotate(90)"/></g>
                    </svg>
                  </span>
                </div>
                <div class="single__accordion-content">
                  <ul>
                    {{ product_description[1] | remove: '</div>' }}
                  </ul>

                  {% comment %}
                    {% if product.metafields.accentuate.fabric_and_care_info %}
                      <p>{{ product.metafields.accentuate.fabric_and_care_info | strip_html }}</p>
                    {% endif %}
                  {% endcomment %}
                </div>
              </div>
            {% endif %}
            {% unless product.title contains 'Pack' or product.title contains 'Tee' or product.title contains 'Gift Card' %}
            <div class="product-single__accordion">
              <div class="product-single__accordion-title active">
                <strong>Save with Bundles</strong>
                <span class="">
                  <svg xmlns="http://www.w3.org/2000/svg" width="17.996" height="2" viewBox="0 0 17.996 2">
                    <g transform="translate(1 1)"><path d="M0,17a1,1,0,0,1-1-1V0A1,1,0,0,1,0-1,1,1,0,0,1,1,0V16A1,1,0,0,1,0,17Z" transform="translate(15.996) rotate(90)"></path></g>
                  </svg>
                </span>
              </div>
              <div class="single__accordion-content active">
                <!-- REBUY SECTION - SAVE WITH BUNDLES - -->
                <div data-rebuy-id="71971" data-rebuy-shopify-product-ids="{{ product.id }}"></div>
                <!-- END REBUY BUNDLES -->
              </div>
            </div>
          

            <style>
              .primary-title {
                display: none;
              }
              .rebuy-widget,
              .rebuy-recommended-products {
                padding: 10px 0 0 0;
              }
            </style>

            {% endunless %}

         {% unless product.title contains 'Gift Card' %}
            <div class="product-single__accordion">
              <div class="product-single__accordion-title active">
                <strong>Pair it With</strong>
                <span class="">
                  <svg xmlns="http://www.w3.org/2000/svg" width="17.996" height="2" viewBox="0 0 17.996 2">
                    <g transform="translate(1 1)"><path d="M0,17a1,1,0,0,1-1-1V0A1,1,0,0,1,0-1,1,1,0,0,1,1,0V16A1,1,0,0,1,0,17Z" transform="translate(15.996) rotate(90)"></path></g>
                  </svg>
                </span>
              </div>
              <div class="single__accordion-content active">
                <!-- REBUY SECTION - PAIR IT WITH - -->
                <div data-rebuy-id="180479" data-rebuy-shopify-product-ids="{{ product.id }}"></div>
                <!-- END REBUY PAIR IT WITH -->
              </div>
            </div>          
          </div>

          <style>
              .primary-title {
                display: none;
              }
              .rebuy-widget,
              .rebuy-recommended-products {
                padding: 10px 0 0 0;
              }
            </style>
       {% endunless %}

          {% if section.settings.social_sharing_products %}
            {% include 'social-sharing',
              share_title: product.title,
              share_permalink: product.url,
              share_image: product.featured_media
            %}
          {% endif %}
        </div>
      </div>
    </div>
  </div>

  {% assign lifestyle_images = product.metafields.accentuate.lifestyle_images.images %}
  {% if lifestyle_images != blank %}
    <div class="product-single-lifestyle-images">
      {% for lifestyle_image in lifestyle_images %}
        <img src="{{lifestyle_image.image}}" alt="{{lifestyle_image.alt}}">
      {% endfor %}
    </div>
  {% endif %}
</div>
{% unless product == empty %}
  <script type="application/json" id="ProductJson-{{ section.id }}">
    {{ product | json }}
  </script>
  <script type="application/json" id="ModelJson-{{ section.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{% endunless %}

{% if product.tags contains 'sock-bundle-eligible' %}
  <style>
    .product-single__quantity {
      display: none;
    }
    .product-single__add-to-cart {
      margin-left: 0px;
    }
    .pack-pricing-flag {
      width: 100%;
      display: none;
      flex-direction: row;
      align-items: center;
      justify-content: flex-start;
      margin-bottom: 25px;
    }
    .pack-pricing-flag:has(#PackSavings:not(:empty)) {
      display: flex;
    }
    .pack-pricing-flag #ComparePrice {
      margin-left: 10px;
    }
    .pack-pricing-flag #PackSavings {
      margin-left: 10px;
    }
    .packsize-single {
      margin-top: 15px;
    }
    .packsize-price--sale {
      margin-top: 5px;
    }

    .single-option-radio label {
      padding: 10px 11px;
      width: 31%;
    }

    #PackSavingsLower {
      display: none;
    }

    .single-option-selector--flag {
      position: absolute;
      top: 0px;
      left: 0px;
      background-color: #f8f8f8;
      font-size: 8px;
    }

    .single-pair {
      margin-top: 19px;
    }

    .single-option-selector-label-eyebrow {
      background: #bb3820;
      color: #fff;
      width: calc(100% + 22px);
      font-size: 12px;
      margin-top: -10px;
      padding: 5px 5px 3px 5px;
      margin-left: -11px;
      margin-bottom: 7px;
    }

    .single-option-selector-value-helper {
      display: flex;
      flex-direction: column;
      align-items: center;
    }
    #PackSavingsLower {
      color: #bb3820;
      font-size: 14px;
      font-weight: 700;
    }
    .pack-pricing-flag #ProductPrice {
      color: #bb3820;
    }
    #PackSavings {
      color: #bb3820;
      font-weight: 700;
    }
    .price-container:has(#PackSavings:not(:empty)) #ProductPrice {
      color: #bb3820;
    }

    @media (max-width: 768px) {
      .packsize-single,
      .packsize-price--sale {
        font-size: 14px;
      }

      .single-option-selector-label-eyebrow {
        font-size: 11px;
      }
    }
  </style>
{% endif %}

<script type="text/javascript">
  var _learnq = _learnq || [];
  var item = {
    Name: {{ product.title|json }},
    ProductID: {{ product.id|json }},
    Categories: {{ product.collections|map:'title'|json }},
    ImageURL: "https:{{ product.featured_image.src|img_url:'grande' }}",
    URL: "{{ shop.secure_url }}{{ product.url }}",
    Brand: {{ product.vendor|json }},
    Price: {{ product.price|money|json }},
    CompareAtPrice: {{ product.compare_at_price_max|money|json }}
  };
  _learnq.push(['track', 'Viewed Product', item]);
  _learnq.push(['trackViewedItem', {
    Title: item.Name,
    ItemId: item.ProductID,
    Categories: item.Categories,
    ImageUrl: item.ImageURL,
    Url: item.URL,
    Metadata: {
      Brand: item.Brand,
      Price: item.Price,
      CompareAtPrice: item.CompareAtPrice
    }
  }]);
</script>

