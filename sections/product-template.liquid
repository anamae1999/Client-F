                    <script>
                      document.addEventListener('DOMContentLoaded', function() {
                        const buttons = document.querySelectorAll('.feature-button');
                      
                        buttons.forEach(button => {
                          button.addEventListener('click', function() {
                            const tooltipId = this.getAttribute('aria-controls');
                            const tooltip = document.getElementById(tooltipId);
                            const isExpanded = this.getAttribute('aria-expanded') === 'true';
                      
                            if (isExpanded) {
                              tooltip.style.visibility = 'hidden';
                              tooltip.style.height = '0px';
                              this.setAttribute('aria-expanded', 'false');
                            } else {
                              tooltip.style.visibility = 'visible';
                              tooltip.style.height = 'auto';
                              this.setAttribute('aria-expanded', 'true');
                            }
                          });

                          if (window.innerWidth > 768) {
                            button.addEventListener('mouseover', function() {
                              const tooltipId = this.getAttribute('aria-controls');
                              const tooltip = document.getElementById(tooltipId);
                              tooltip.style.visibility = 'visible';
                              tooltip.style.height = 'auto';
                              this.setAttribute('aria-expanded', 'true');
                            });
                        
                            button.addEventListener('mouseout', function() {
                              const tooltipId = this.getAttribute('aria-controls');
                              const tooltip = document.getElementById(tooltipId);
                              tooltip.style.visibility = 'hidden';
                              tooltip.style.height = '0px';
                              this.setAttribute('aria-expanded', 'false');
                            });
                          }
                      
                          button.addEventListener('focus', function() {
                            const tooltipId = this.getAttribute('aria-controls');
                            const tooltip = document.getElementById(tooltipId);
                            tooltip.style.visibility = 'visible';
                            tooltip.style.height = 'auto';
                            this.setAttribute('aria-expanded', 'true');
                          });
                      
                          button.addEventListener('blur', function() {
                            const tooltipId = this.getAttribute('aria-controls');
                            const tooltip = document.getElementById(tooltipId);
                            tooltip.style.visibility = 'hidden';
                            tooltip.style.height = '0px';
                            this.setAttribute('aria-expanded', 'false');
                          });
                      
                          button.addEventListener('keydown', function(event) {
                            if (event.key === 'Escape') {
                              const tooltipId = this.getAttribute('aria-controls');
                              const tooltip = document.getElementById(tooltipId);
                              tooltip.style.visibility = 'hidden';
                              tooltip.style.height = '0px';
                              this.setAttribute('aria-expanded', 'false');
                            }
                          });
                        });
                      });

                       // Add event listeners for close buttons inside tooltips
                        const closeButtons = document.querySelectorAll('.close-tooltip');
                        closeButtons.forEach(closeButton => {
                          closeButton.addEventListener('click', function(event) {
                            const tooltip = closeButton.closest('.tooltip');
                            const buttonId = tooltip ? tooltip.getAttribute('id').replace('tooltip-', 'button-') : null;
                            const button = buttonId ? document.getElementById(buttonId) : null;

                            if (tooltip && button) {
                              hideTooltip(button, tooltip);
                            }

                            // Prevent the click event from bubbling up and triggering the button's click event.
                            event.stopPropagation();
                          });
                        });

                        function showTooltip(button, tooltip) {
                          tooltip.style.visibility = 'visible';
                          tooltip.style.height = 'auto';
                          button.setAttribute('aria-expanded', 'true');
                        }

                        function hideTooltip(button, tooltip) {
                          tooltip.style.visibility = 'hidden';
                          tooltip.style.height = '0px';
                          button.setAttribute('aria-expanded', 'false');
                        }
                     
                    </script>

