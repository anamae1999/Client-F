<!-- /templates/product.liquid -->
{%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}

<div
  id="ProductSection--{{ section.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="product-template"
  data-image-zoom-type="{{ section.settings.enable_image_zoom }}"
  data-enable-history-state="true"
  data-stacked-layout="{% if section.settings.media_layout == "stacked" %}true{% else %}false{% endif %}"
  {% if first_3d_model %}
    data-has-model="true"
  {% endif %}
>
  <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
  <meta itemprop="image" content="{{ product.featured_media | img_url: 'grande' }}">

  <div id="defaultWomensColor" class="hide">{{ product.metafields.accentuate.womens_default_color }}</div>
  <div id="defaultMensColor" class="hide">{{ product.metafields.accentuate.mens_default_color }}</div>

  {% assign current_variant = product.selected_or_first_available_variant %}
  {% assign featured_media = current_variant.featured_media | default: product.featured_media %}

  {% assign stacked = false %}
  {% if section.settings.media_layout == 'stacked' %}{% assign stacked = true %}{% endif %}
  {%- assign first_media = true -%}

  <div class="wrapper">
    <div class="productGrid product-single">
      <div data-mobile-only>
        {% unless product.gift_card? %}
          {% render 'breadcrumbs' %}
        {% endunless %}

        {% if section.settings.product_vendor_enable %}
          <h2 class="product-single__vendor" itemprop="brand">{{ product.vendor }}</h2>
        {% endif %}

        <div class="product-single__headline">
          <h1 class="product-single__title" itemprop="name">{{ product.title }}</h1>
          <div class="yotpo bottomLine" data-product-id="{{ product.id }}"></div>
        </div>
      </div>

      {% render 'product-gallery--listo', product: product %}

      <div class="productGridItem--topContent">
        <div data-desktop-only>
          {% unless product.gift_card? %}
            {% render 'breadcrumbs' %}
          {% endunless %}

          {% if section.settings.product_vendor_enable %}
            <h2 class="product-single__vendor" itemprop="brand">{{ product.vendor }}</h2>
          {% endif %}

          <div class="product-single__headline">
            <h1 class="product-single__title" itemprop="name">{{ product.title }}</h1>

            <div class="yotpo bottomLine" data-product-id="{{ product.id }}"></div>
          </div>
        </div>
        {% comment %}
          Optionally show the 'compare at' or original price of the product.
        {% endcomment %}
        {% render 'product-price', variant: current_variant %}

        {% comment %} LISTO - Theme Discount - START {% endcomment %}
        {% comment %}
          {% if settings.sale_collection_handle != blank %}
            {% assign show_on_scoped_collection = true %}

            {% for collection in product.collections %}
              {% if collection.handle == settings.sale_collection_handle %}
                FOUND!
                {% break %}
              {% else %}

              {% endif %}
            {% endfor %}
          {% endif %}
        {% endcomment %}

        {% if settings.show_pdp_sale_text %}

          {% liquid
            assign sale-message = "after 20% off discount at checkout"
            
            if product.title contains '3' or product.title contains 'Three'
              assign sale-message = "after 22% off discount at checkout"
            elsif product.title contains '6' or product.title contains 'Six'
              assign sale-message = "after 25% off discount at checkout"
            endif
          %}
          {% unless product.title == 'Gift Card' %}
            {% assign theme_discount_percent = settings.sale_discount_percent %}
            {% assign theme_price_multiplier = 100 | minus: theme_discount_percent | divided_by: 100.0 %}
            <div data-theme-discount-info>
              <span data-discounted-price>
                {{ product.selected_or_first_available_variant.price | times: theme_price_multiplier | money -}}</span
              ><span data-discount-amount> {{ sale-message }}</span>
            </div>
            <style>
              .price-container {
                margin-bottom: 0px
              }
              .product-single__price {
                text-decoration: line-through;
                color:  #646464;
              }
              [data-theme-discount-info] {
                color: #bb3820;
              }
              [data-theme-discount-info] [data-discounted-price] {
                font-size: 20px;
                font-weight: 700;
              }
            </style>
            <script
              type="text/javascript"
              data-discount-percentage="{{ settings.sale_discount_percent }}"
            >
              let themeDiscountPercentage = document
                .querySelector('script[data-discount-percentage]')
                .getAttribute('data-discount-percentage');
              let priceMultiplier = (100 - themeDiscountPercentage) / 100;
              let productPriceSpan = document.querySelector('[data-price-container] #ProductPrice');

              // Options for the observer (which mutations to observe)
              const config = { attributes: true, childList: true, subtree: true };

              // Callback function to execute when mutations are observed
              const callback = (mutationList, observer) => {
                for (const mutation of mutationList) {
                  if (mutation.type === 'childList') {
                    let comparePriceSpan = document.querySelector('[data-price-container] #ComparePrice');
                    if (comparePriceSpan && comparePriceSpan.innerHTML.trim()) {
                     
                      if (document.querySelector('.single-option-selected-value.pair-amount').innerHTML.includes('3')) {
                        let priceSpan = document.querySelector('[data-price-container] #ComparePrice');
                        let newPrice = parseFloat(priceSpan.innerHTML.replace(/[^0-9.]/g, ''));
                        let newSalePrice = (newPrice * 0.78).toFixed(2);
                        let displayPriceSpan = document.querySelector(
                          '[data-theme-discount-info] span[data-discounted-price]'
                        );
                        let discountMessage = document.querySelector('[data-theme-discount-info] span[data-discount-amount]');
                        discountMessage.innerHTML = ' after 22% off discount at checkout';
                        displayPriceSpan.innerHTML = '$' + newSalePrice;
                      }

                      if (document.querySelector('.single-option-selected-value.pair-amount').innerHTML.includes('6')) {
                        let priceSpan = document.querySelector('[data-price-container] #ComparePrice');
                        let newPrice = parseFloat(priceSpan.innerHTML.replace(/[^0-9.]/g, ''));
                        let newSalePrice = (newPrice * 0.75).toFixed(2);
                        let displayPriceSpan = document.querySelector(
                          '[data-theme-discount-info] span[data-discounted-price]'
                        );
                        let discountMessage = document.querySelector('[data-theme-discount-info] span[data-discount-amount]');
                        discountMessage.innerHTML = ' after 25% off discount at checkout';
                        displayPriceSpan.innerHTML = '$' + newSalePrice;
                      }

                      if (document.querySelector('.single-option-selected-value.pair-amount').innerHTML.includes('1')) {
                        let priceSpan = document.querySelector('[data-price-container] #ProductPrice');
                        let newPrice = parseFloat(priceSpan.innerHTML.replace(/[^0-9.]/g, ''));
                        let newSalePrice = (newPrice * 0.8).toFixed(2);
                        let displayPriceSpan = document.querySelector(
                          '[data-theme-discount-info] span[data-discounted-price]'
                        );
                        let discountMessage = document.querySelector('[data-theme-discount-info] span[data-discount-amount]');
                        discountMessage.innerHTML = ' after 20% off discount at checkout';
                        displayPriceSpan.innerHTML = '$' + newSalePrice;
                      }
                      
                    } else {
                    
                    let priceSpan = document.querySelector('[data-price-container] #ProductPrice');
                    let newPrice = parseFloat(priceSpan.innerHTML.replace(/[^0-9.]/g, ''));
                    let newSalePrice = (newPrice * priceMultiplier).toFixed(2);
                    let displayPriceSpan = document.querySelector(
                      '[data-theme-discount-info] span[data-discounted-price]'
                    );
                      displayPriceSpan.innerHTML = '$' + newSalePrice;
                    }
                  }
                }
              };

              // Create an observer instance linked to the callback function
              const observer = new MutationObserver(callback);

              // Start observing the target node for configured mutations
              observer.observe(productPriceSpan, config);
            </script>
          {% endunless %}
        {% endif %}

        {% comment %} LISTO - Theme Discount - END {% endcomment %}

        {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
          <div class="product-single__policies rte">
            {%- if shop.taxes_included -%}
              {{ 'products.general.include_taxes' | t }}
            {%- endif -%}
            {%- if shop.shipping_policy.body != blank -%}
              {{ 'products.general.shipping_policy_html' | t: link: shop.shipping_policy.url }}
            {%- endif -%}
          </div>
        {%- endif -%}
        <div class="product-single__meta">
          <meta itemprop="priceCurrency" content="{{ cart.currency.iso_code }}">

          {% capture "form_classes" %}
              product-single__form{% if product.has_only_default_variant %} product-single__form--no-variants{% endif %}
            {%- endcapture %}

          {% capture "form_id" %}AddToCartForm--{{ section.id }}{%- endcapture %}

          {% form 'product', product, class: form_classes, id: form_id, data-product-form: '' %}
            {% unless product.has_only_default_variant %}
              {% for option in product.options_with_values %}
                {% comment %}
                  Make variant option name consistent
                  (in shopify-admin, some are named "Size" while others are "Size Description", etc)
                {% endcomment %}
                {% assign optionNameLowercase = option.name | downcase %}
                {% if optionNameLowercase contains 'size' %}
                  {% assign optionName = 'Size' %}
                {% elsif optionNameLowercase contains 'color' %}
                  {% assign optionName = 'Color' %}
                {% else %}
                  {% assign optionName = option.name %}
                {% endif %}

                {% unless product.title contains 'Tee' %}

                {% if optionName == 'Size' %}
                  {% unless product.title contains 'Men' or product.title contains 'Women' or product.gift_card? %}
                    <div class="product-single-gender-toggle">
                      <span
                        data-gender="women"
                        class="gender-toggle-item{% if collection %}{% if collection.handle contains 'womens-' %} gender-toggle-item--selected{% endif %}{% else %} gender-toggle-item--selected{%endif %}">Women's Size</span>
                        <span
                          data-gender="men"
                          class="gender-toggle-item{% if collection %}{% unless collection.handle contains 'womens-' %} gender-toggle-item--selected{% endunless %}{% endif %}"
                          >Men's Size</span>
                    </div>
                  {% endunless %}
                {% endif %}

                {% endunless %}
                <div class="radio-wrapper js product-form__item">
                  {%  if product.title contains 'Tee' and optionName == 'Size' %}
                    <div class="size-title-wrapper">
                  {% endif %}
                  <label
                    class="single-option-radio__label {{ optionName }}"
                    for="ProductSelect-option-{{ forloop.index0 }}"
                  >
                    {{ optionName | escape }}
                    <span class="single-option-selected-value">{{ option.selected_value | downcase }}</span>
                    <span
                      class="single-option-selected-value-helper {% if forloop.first %}  visually-hidden{% endif %}"
                    >
                    </span>
                  </label>
                  {% if product.title contains 'Tee' and optionName == 'Size' %}
                    <button type="button" id="size-guide" class="size-guide-button">Size Guide</button>
                    <dialog class="size-guide-dialog" id="size-guide-dialog">
                      <div id="size-guide-div">{% if product.handle contains 'womens' %}{{ pages["size-chart-mens-performance-tee"].content }}{% else %}{{ pages["size-chart-mens-performance-tee"].content }}{% endif %}</div>
                    </dialog>
                    <script>
                      const sizeGuideButton = document.getElementById('size-guide');
                      sizeGuideButton.addEventListener('click', () => sizeGuideDialog.showModal());
        
                      const sizeGuideDialog = document.getElementById('size-guide-dialog');
                      sizeGuideDialog.addEventListener('click', () => sizeGuideDialog.close());
        
                      const sizeGuideDiv = document.getElementById('size-guide-div');
                      sizeGuideDiv.addEventListener('click', (event) => event.stopPropagation());
                    </script>
                    
                  <style>
                    .size-title-wrapper {
                      display: flex;
                      justify-content: space-between;
                      align-items: start;
                    }
                     .atc-feature-dialog:-internal-dialog-in-top-layer::backdrop {
                background: #000000ab;
              }

              .size-guide-dialog:before {
                content: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M23 20.168l-8.185-8.187 8.185-8.174-2.832-2.807-8.182 8.179-8.176-8.179-2.81 2.81 8.186 8.196-8.186 8.184 2.81 2.81 8.203-8.192 8.18 8.192z"/></svg>');
                font-size: 2rem;
                position: absolute;
                top: 15px;
                right: 15px;
                line-height: 1rem;
              }

              .size-guide-dialog {
                border: none;
              }

              .size-guide-dialog:before:hover {
                cursor: pointer;
              }

      
          

              .size-guide-button {
                font-size: 14px;
                text-decoration: underline;
                padding: 0px;
              }

              .size-guide-button:hover {
                opacity: 0.6;
              }

              .size-guide-dialog div {
              padding: 20px;
              padding-top: 50px;
            }


              .size-guide-dialog {
                width: fit-content;
                height: fit-content;
                max-width: 700px;
                max-height: 700px;
              }

          

              @media (max-width: 767px) {
                .size-guide-dialog {
                  max-width: 100%;
                }
              }

                </style>
                  {% endif %}

                 {%  if product.title contains 'Tee' and optionName == 'Size' %}
                   </div>
                  {% endif %}

                
                  {% if section.settings.product_selector == 'radio' %}
                    <div
                      class="single-option-radio {{ optionName }}"
                      id="ProductSelect-option-{{ forloop.index0 }}"
                    >
                      {% assign option_index = forloop.index %}
                      {% for value in option.values %}
                        {% assign value_lower_cased = value | downcase %}
                        {% assign variant_label_state = true %}
                        {% if product.options.size == 1 %}
                          {% unless product.variants[forloop.index0].available %}
                            {% assign variant_label_state = false %}
                          {% endunless %}
                        {% endif %}
                        <input
                          type="radio"
                          {% if option.selected_value == value %}
                            checked="checked"
                          {% endif %}
                          {% unless variant_label_state %}
                            disabled="disabled"
                          {% endunless %}
                          value="{{ value | escape }}"
                          data-index="option{{ option_index }}"
                          name="option{{ option.position }}"
                          class="single-option-selector__radio{% unless variant_label_state %} disabled{% endunless %}"
                          data-option-name="{{optionName}}"
                          data-foobar="{{optionName}}"
                          id="ProductSelect-option-{{ optionName | handleize | replace: " ","-" }}-{{ value | escape | replace: " ","-" }}"
                        >
                        <label
                          class="single-option-selector__label {{ optionName }}-{{ value }}"
                          for="ProductSelect-option-{{ optionName | handleize | replace: " ","-" }}-{{ value | escape | replace: " ","-" }}"
                          {% unless variant_label_state %}
                            class="disabled"
                          {% endunless %}
                          data-variant-value="{{value}}"
                          {% for variant in product.variants %}
                            {% if variant.option1 == value %}
                              {% if variant.metafields.accentuate.exclusively_women %}
                                data-exclusive-gender="women"
                              {% endif %}
                              {% if variant.metafields.accentuate.exclusively_men %}
                                data-exclusive-gender="men"
                              {% endif %}
                              {% if variant.metafields.accentuate.new %}
                                data-new
                              {% endif %}
                              {% if variant.compare_at_price > variant.price %}
                                data-on-sale
                              {% endif %}
                            {% endif %}
                          {% endfor %}
                        >
                          {% if product.gift_card? %}
                            {{ value }}
                          {% endif %}

                          {% if optionNameLowercase contains 'color' %}
                            {% assign count = 0 %}
                            {% for image in product.images %}
                              {% if image.alt == value %}
                                {% if count < 1 %}
                                  {% assign count = count | plus: 1 %}
                                  <img class="" loading="lazy" src="{{image | img_url: '176x176'}}" alt="{{image.alt}}">
                                {% endif %}
                              {% endif %}
                            {% endfor %}
                            {% if count == 0 %}
                              {{ value | escape }}
                            {% endif %}
                          {% endif %}

                          {% if optionNameLowercase contains 'size' %}
                            {% if value contains 'X' or value contains 'x' %}
                              {{ value | escape | slice: 0, 2 }}
                            {% else %}
                              {{ value | escape | slice: 0 }}
                            {% endif %}
                          {% endif %}
                          {% unless value_lower_cased == 'xlarge' %}
                            <span class="sizes-women">
                              {% if value_lower_cased == 'small' %}
                                4-6.5
                              {% elsif value_lower_cased == 'medium' %}
                                7-9.5
                              {% elsif value_lower_cased == 'large' %}
                                10-13
                              {% endif %}
                            </span>
                          {% endunless %}
                          {% unless value_lower_cased == 'small' %}
                            <span class="sizes-men">
                              {% if value_lower_cased == 'medium' %}
                                6-8.5
                              {% elsif value_lower_cased == 'large' %}
                                9-12
                              {% elsif value_lower_cased == 'xlarge' %}
                                12.5-15.5
                              {% endif %}
                            </span>
                          {% endunless %}
                        </label>
                      {% endfor %}
                    </div>
                  {% else %}
                    <select
                      class="single-option-selector__radio single-option-selector-{{ section.id }} product-form__input"
                      id="SingleOptionSelector-{{ forloop.index0 }}"
                      data-index="option{{ forloop.index }}"
                    >
                      {% for value in option.values %}
                        <option
                          value="{{ value | escape }}"
                          {% if option.selected_value == value %}
                            selected="selected"
                          {% endif %}
                        >
                          {{ value | escape }}
                        </option>
                      {% endfor %}
                    </select>
                  {% endif %}
                </div>
              {% endfor %}

              {% comment %} LISTO - PACK PRICING BUTTONS - START {% endcomment %}
              {% if product.tags contains 'sock-bundle-eligible' %}
                <div class="radio-wrapper js product-form__item">
                  <label
                    class="single-option-radio__label"
                    for="ProductSelect-packsize"
                  >
                    Pack Size
                    <strong class="single-option-selected-value pair-amount">1 Pair</strong>
                    <strong class="single-option-selected-value-helper"></strong>
                    <span id="PackSavingsLower"></span>
                  </label>
                  <div class="single-option-radio" id="ProductSelect-packsize">
                    <input
                      type="radio"
                      value="1 Pair"
                      data-qty="1"
                      data-index="optionsize"
                      name="optionsize"
                      class="pack-size-selector__radio"
                      data-foobar="packsize"
                      id="ProductSelect-packsize-single"
                      checked="checked"
                    >
                    <label
                      class="single-option-selector__label"
                      for="ProductSelect-packsize-single"
                      data-variant-value=""
                    >
                      <div class="single-option-selector-value single-pair">1 Pair</div>
                      <div class="single-option-selector-value-helper">
                        <span
                          aria-label="1 pack compare at price {{- current_variant.compare_at_price | money -}}"
                          class="packsize-price--compare"
                        >
                          {% if current_variant.compare_at_price and current_variant.compare_at_price > current_variant.price %}
                            {{- current_variant.compare_at_price | money -}}
                          {% endif %}
                        </span>
                        <span aria-label="Single pair price" class="packsize-price packsize-price--sale">
                          {{- current_variant.price | money -}}
                        </span>
                      </div>
                    </label>
                    <input
                      type="radio"
                      value="3 Pack"
                      data-qty="3"
                      data-index="optionsize"
                      name="optionsize"
                      class="pack-size-selector__radio"
                      data-foobar="packsize"
                      id="ProductSelect-packsize-three"
                    >
                    <label
                      class="single-option-selector__label"
                      for="ProductSelect-packsize-three"
                      data-variant-value=""
                    >
                      <div class="single-option-selector-label-eyebrow">MOST POPULAR</div>
                      <div class="single-option-selector-value pack-amount">3 Pack</div>
                      <div class="single-option-selector-value-helper">
                        <span
                          aria-label="3 pack compare at price {{- current_variant.price | times: 3 | money -}}"
                          class="packsize-price packsize-price--compare"
                        >
                          {{- current_variant.price | times: 3 | money -}}
                        </span>
                        <span
                          aria-label="3 pack discounted price  {{- current_variant.price | times: 3 | times: 0.95 | money -}}"
                          class="packsize-price packsize-price--sale"
                          data-savings-percent="5"
                        >
                          {{- current_variant.price | times: 3 | times: 0.95 | money -}}
                        </span>
                      </div>
                    </label>
                    <input
                      type="radio"
                      value="6 Pack"
                      data-qty="6"
                      data-index="optionsize"
                      name="optionsize"
                      class="pack-size-selector__radio"
                      data-foobar="packsize"
                      id="ProductSelect-packsize-six"
                    >
                    <label class="single-option-selector__label" for="ProductSelect-packsize-six" data-variant-value="">
                      <div class="single-option-selector-label-eyebrow">BEST VALUE</div>
                      <div class="single-option-selector-value pack-amount">6 Pack</div>
                      <div class="single-option-selector-value-helper">
                        <span
                          aria-label="6 pack compare at price  {{- current_variant.price | times: 6 | money -}}"
                          class="packsize-price packsize-price--compare"
                        >
                          {{- current_variant.price | times: 6 | money -}}
                        </span>
                        <span
                          aria-label="6 pack discounted price {{- current_variant.price | times: 6 | times: 0.9 | money -}}"
                          class="packsize-price packsize-price--sale"
                          data-savings-percent="10"
                        >
                          {{- current_variant.price | times: 6 | times: 0.9 | money -}}
                        </span>
                      </div>
                    </label>
                  </div>
                </div>
              {% endif %}
              {% comment %} LISTO - PACK PRICING BUTTONS - END {% endcomment %}
            {% endunless %}

            <select
              name="id"
              id="ProductSelect--{{ section.id }}"
              class="product-single__variants visually-hidden no-js"
              aria-label="hidden list of all skus with prices"
            >
              {% assign variant_color_checked = '' %}
              {% for variant in product.variants %}
                {% if variant.inventory_quantity > 6 or product.handle == 'gift-card' %}
                  <option
                    {% if variant == product.selected_or_first_available_variant %}
                      selected="selected"
                    {% endif %}
                    data-sku="{{ variant.sku }}"
                    data-variant-color="{{ variant.option1 | replace: ' ', '-' }}"
                    data-variant-size="{{ variant.option2 }}"
                    value="{{ variant.id }}"
                  >
                    {{ variant.title }} - {{ variant.price | money_with_currency }}
                  </option>
                {% else %}
                  <option
                    disabled="disabled"
                    data-sku="{{ variant.sku }}"
                    data-variant-color="{{ variant.option1 | replace: ' ', '-' }}"
                    data-variant-size="{{ variant.option2 }}"
                    value="{{ variant.id }}"
                  >
                    {{ variant.title }} - {{ 'products.product.sold_out' | t }}
                  </option>
                {% endif %}
                {% unless variant.option1 == variant_color_checked %}
                  {% assign all_variant_sizes = product.variants | where: 'option1', variant.option1 %}
                  {% if all_variant_sizes.size < 4 %}
                    {% assign sizes_to_add = 'small medium large xlarge ' %}
                    {% for variant in all_variant_sizes %}
                      {% assign size_downcase = variant.option2 | downcase %}
                      {% if size_downcase == 'small' %}
